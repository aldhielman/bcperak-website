        "drupal/entity_panels": "^1.2",
        "drupal/entityreference": "^1.5",
        "drupal/faq": "^1.3",
        "drupal/features": "^2.13",
        "drupal/field_collection": "^1.2",
        "drupal/field_collection_fieldset": "^2.6",
        "drupal/field_collection_table": "^1.0@beta",
        "drupal/field_collection_tabs": "^1.1",
        "drupal/field_collection_views": "^1.0@beta",
        "drupal/field_conditional_state": "^2.1",
        "drupal/field_formatter_settings": "^1.1",
        "drupal/field_group": "^1.6",
        "drupal/field_slideshow": "^1.83",
        "drupal/field_validation": "^2.6",
