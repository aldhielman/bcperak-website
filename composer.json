        "drupal/filefield_sources": "^1.11",
        "drupal/flood_control": "^1.0",
        "drupal/grid_field_formatter": "^1.1",
        "drupal/hide_submit": "^2.4",
        "drupal/i18n": "^1.31",
        "drupal/image_link_formatter": "^1.1",
        "drupal/imce": "^1.11",
        "drupal/imce_filefield": "^1.1",
        "drupal/imce_mkdir": "^1.0",
        "drupal/imce_rename": "^1.3",
        "drupal/imce_wysiwyg": "^1.0",
        "drupal/ip_ranges": "^1.2",
        "drupal/jcarousel": "^2.7",
        "drupal/jquery_update": "^2.7",
        "drupal/js_injector": "^2.1",
        "drupal/l10n_update": "^2.4",
        "drupal/libraries": "^2.5",
        "drupal/link": "^1.9",
