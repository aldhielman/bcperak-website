        "drupal/path2ban": "^2.3",
        "drupal/pathauto": "^1.3",
        "drupal/pdf": "^1.9",
        "drupal/pdf_to_imagefield": "^3.4",
        "drupal/pdfpreview": "^2.1",
        "drupal/permission_select": "^1.2",
        "drupal/phpmailer": "^3.1",
        "drupal/popup_question_redirect": "^1.3",
        "drupal/profile2": "^2.0",
        "drupal/quicktabs": "^3.8",
        "drupal/realname": "^1.4",
        "drupal/redirect": "^1.0@RC",
